  "memory": {
    "format": "ğŸ’¾{used:0.1f}/{total:0.1f} GB",
    "align": "center",
  },
  "disk": {
    "interval": 60,
    "format": "â› {percentage_used}%",
    "align": "left",
  },
  "battery": {
    "bat": "BAT0",
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15,
    },
    "format": "ğŸ”‹{capacity}%",
    "format-charging": "âš¡{capacity}%",
