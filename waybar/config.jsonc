{
  "modules-left": ["hyprland/workspaces"],
  "modules-center": ["hyprland/window"],
  "modules-right": [
    "network#speed",
    "tray",
    "memory",
    "disk",
    "pulseaudio",
    "battery",
    "clock",
  ],
  "hyprland/window": {
    "format": "{initialTitle}",
    "swap-icon-label": true,
  },
  "network#speed": {
    "font-family": "JetBrainsMono Nerd Font monospace",
    "interface": "wlan0",
    "interval": 1,
    "format": "{ifname}",
    "format-wifi": "ï„º  {bandwidthDownBytes:>6} ï„¹  {bandwidthUpBytes:>6} {icon}",
    "format-ethernet": "ï„º  {bandwidthDownBytes:>6}  ï„¹  {bandwidthUpBytes:>6} {icon}",
    "format-disconnected": "ó°Œ™",
    "tooltip-format": "{ifname : ipaddr}",
    "format-linked": "ó°ˆ {ifname} (No IP)",
    "tooltip-format-wifi": "{essid} {icon} {signalStrength}%",
    "tooltip-format-ethernet": "{ifname} ó°Œ˜",
    "tooltip-format-disconnected": "ó°Œ™ Disconnected",
    "min-length": 25,
    "max-length": 25,
    "format-icons": ["ó°¤¯", "ó°¤Ÿ", "ó°¤¢", "ó°¤¥", "ó°¤¨"],
  },

  "hyprland/workspaces": {
    "format": "{icon}  {windows}",
